@model List<Restaurant.Models.RestaurantViewModel>
@if (Model?.Count > 0)
{
    <div>
        <div>
            <table class="table">
                <thead>
                    <tr>
                        <th style="display:none">
                            Restaurant Id
                        </th>
                        <th>
                            Restaurant Name
                        </th>
                        <th>
                            Available Time
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td style="display:none">
                                @Html.DisplayFor(modelItem => item.RestaurantId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.RestaurantName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FromTime) - @Html.DisplayFor(modelItem => item.ToTime)
                            </td>
                            @*<td>
                                @Html.DisplayFor(modelItem => item.FromTime)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ToTime)
                            </td>*@
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
}